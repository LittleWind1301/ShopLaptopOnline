@model List<ModelWeb.EF1.Menu>


<div class="call">
    <p><span>Need help?</span> call us <span class="number">1-22-3456789</span></span></p>
</div>
<div class="account_desc">
    <ul>
        @*<li><a  href="/Admin/Login/Login">Admin</a></li>*@
        @foreach (var item in Model)
        {
            <li><a id="top_menu" href="@item.Link" target="@item.Target">@item.Text</a></li>

        }
        @if (Session["UserName"] != null)
        {
            <br />
            <span class="mr-2 d-none d-lg-inline text-gray-600">Hello: @Session["UserName"]</span>
            @Html.ActionLink("Logout", "DangXuat", "Customer")
        }
    </ul>
</div>
<div class="clear"></div>
